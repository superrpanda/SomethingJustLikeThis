  if test "$enableval" = "yes"; then
    MOZ_GFX_OPTIMIZE_MOBILE=1
  elif test "$enableval" = "no"; then
    :
  else
    { echo "configure: error: Option, mobile-optimize, does not take an argument ($enableval)." 1>&2; echo "configure: error: Option, mobile-optimize, does not take an argument ($enableval)." 1>&5; exit 1; }
  fi
