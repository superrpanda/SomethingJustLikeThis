    MOZ_FIX_LINK_PATHS="-Wl,-executable_path,${DIST}/bin"
    ;;

*-android*|*-linuxandroid*)
    if test "$COMPILE_ENVIRONMENT" -a -n "$MOZ_MEMORY"; then
        MOZ_LINKER=1
    fi

    MOZ_GFX_OPTIMIZE_MOBILE=1
